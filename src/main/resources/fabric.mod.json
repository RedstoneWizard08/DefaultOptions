{
	"schemaVersion": 1,
	"id": "defaultoptions",
	"version": "${version}",
	"name": "DefaultOptions",
	"description": "A simple mod for modpack devs to load a default options.txt if it doesn't already exist.",
	"authors": [
		"RedstoneWizard08"
	],
	"contact": {
		"homepage": "https://github.com/RedstoneWizard08/DefaultOptions",
		"sources": "https://github.com/RedstoneWizard08/DefaultOptions"
	},
	"license": "CC0-1.0",
	"icon": "assets/defaultoptions/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"redstonedev.defaultoptions.DefaultOptions"
		],
		"preLaunch": [
			"redstonedev.defaultoptions.DefaultOptionsPreLaunch"
		]
	},
	"mixins": [
		"defaultoptions.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.2",
		"minecraft": "~1.21.1",
		"java": ">=21",
		"fabric-api": "*"
	}
}